{
    "streaming" : {
        "interval" : 5,
        "service" : "pubsub",
        "connection_creds" : {
            "project_id": "my-gcp-project",
            "topic": "my_pubsub_topic",
            "credentials_path": "/path/to/service_account.json"
        }
    },
    "batch" : {
        "file_name" : "data_batch",
        "interval" : 900
    },
    "data_description": [
      {
        "name": "sensor_id",
        "data_type": "category",
        "allowable_values": ["sensor_1", "sensor_2", "sensor_3"]
      },
      {
        "name": "value",
        "data_type": "numeric",
        "allowable_values": [0.0, 100.0] 
      }
    ]            
  }
  